openshift.withCluster() { 
    echo "Hello from the project running Jenkins: ${openshift.project()}"
    def dupaSelector = openshift.selector( 'pod' )
    echo "mamy ${dupaSelector.count()} podow w  projekcie ${openshift.project()}"
    echo "nazywaja sie: ${dupaSelector.names()}"
    echo "------bleble------------"
    dupaSelector.withEach {
        echo "POD: ${it.name()} is defined in ${openshift.project()}"
    }

}
